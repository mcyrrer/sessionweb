<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<!--Test suite for running all tests -->
<suite name="SuiteAll" verbose="1">
    <test name="TestAll"><!--
        <groups>
            <run>
                <include name="Get"/>
                <include name="Put"/>
                <include name="Post"/>
                <include name="Delete"/>
            </run>
        </groups>
        --><packages>
            <package name="com.sessionweb.autotest.tests.*"/>
        </packages>
    </test>
</suite>
